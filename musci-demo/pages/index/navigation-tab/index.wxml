<view class="navigation-tab">
  <view wx:for="{{tabList}}"
        wx:for-item="tabItem"
        wx:key="id"
        class="tab-item {{activeTabId === tabItem.id ? 'active' : ''}}"
        data-tab="{{tabItem}}"
        bindtap="onClickTab">
    <text class="tab-title">{{tabItem.title}}</text>
  </view>
</view>
